# AI Learning Platform - Настройки окружения
# Скопируйте этот файл в .env и измените значения

# База данных
DATABASE_URL=postgresql://user:password@localhost:5432/ai_learning_platform

# Безопасность
SECRET_KEY=your-super-secret-key-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# OpenAI настройки по умолчанию
DEFAULT_OPENAI_API_KEY=your-openai-api-key-here
DEFAULT_OPENAI_MODEL=gpt-3.5-turbo
DEFAULT_OPENAI_BASE_URL=https://api.openai.com/v1

# Настройки приложения
APP_NAME=AI Learning Platform
DEBUG=false

# CORS настройки
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Дополнительные настройки
LOG_LEVEL=info 